[program:laravel-worker]
process_name=%(program_name)s_%(process_num)02d
command=php /var/www/artisan queue:work --sleep=3 --tries=3 --timeout=300
autostart=true
startsecs = 0
autorestart=true
numprocs=2
user=root
stdout_logfile=/var/www/storage/logs/laravel-worker-stdout.log
stderr_logfile=/var/www/storage/logs/laravel-worker-stderr.log
redirect_stderr=true